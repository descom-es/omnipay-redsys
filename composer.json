{
    "name": "descom/omnipay-redsys",
    "description": "Redsys driver for Omnipay",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\Redsys\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Redsys\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Cesar",
            "email": "cesar@descom.es"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.8",
        "league/omnipay": "^3.2",
        "omnipay/common": "^3.2",
        "descom/redsys": "^1.9"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1|^2",
        "omnipay/tests": "^4.1",
        "phpunit/phpunit": "^10.5|^11.5",
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}